{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "disableSort": true,
    "applicationName": "GIOŚ API Wrapper",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "GIOŚ API JavaScript Wrapper",
      "type": "website",
      "image": "",
      "site_name": "GIOŚ API",
      "url": ""
    },
    "meta": {
      "title": "GIOŚ API JavaScript Wrapper",
      "description": "GIOŚ API JavaScript Wrapper",
      "keyword": "gioś, api, wrapper, javascript, js"
    },
    "linenums": true
  },
  "opts": {
    "destination": "docs",
    "encoding": "utf8",
    "recurse": true,
    "private": true,
    "lenient": true,
    "template": "./node_modules/@pixi/jsdoc-template"
  }
}
