{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "disableSort": true,
    "referenceTitle": "GIOÅš API Wrapper",
    "collapse": false,
    "resources": {
      "GitHub": "https://github.com/mixon00/gios-api"
    }
  },
  "opts": {
    "destination": "docs",
    "encoding": "utf8",
    "recurse": true,
    "private": true,
    "template": "./node_modules/braintree-jsdoc-template"
  }
}
